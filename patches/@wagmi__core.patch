+/**
+ * Serializes a WebAuthn credential for browsers that don't support toJSON().
+ * Safari < 18.4 lacks PublicKeyCredential.toJSON(), causing JSON.stringify to return {}.
+ * This helper manually serializes the credential response fields.
+ */
+function serializeCredential(credential: WebAuthnP256.P256Credential['raw']): unknown {
+  if (!credential) return credential
+
+  // If toJSON exists (Safari 18.4+, Chrome, Firefox), use native serialization
+  if (typeof (credential as any).toJSON === 'function') {
+    return (credential as any).toJSON()
+  }
+
+  // Manual serialization for older browsers (Safari < 18.4)
+  const base64url = (buffer: ArrayBuffer): string => {
+    const bytes = new Uint8Array(buffer)
+    let str = ''
+    for (const byte of bytes) str += String.fromCharCode(byte)
+    return btoa(str).replace(/\+/g, '-').replace(/\//g, '_').replace(/=+$/, '')
+  }
+
+  return {
+    id: credential.id,
+    type: credential.type,
+    authenticatorAttachment: (credential as any).authenticatorAttachment,
+    clientExtensionResults: (credential as any).getClientExtensionResults?.() ?? {},
+    response: {
+      clientDataJSON: base64url(credential.response.clientDataJSON),
+      attestationObject: base64url((credential.response as any).attestationObject),
+      authenticatorData: (credential.response as any).getAuthenticatorData
+        ? base64url((credential.response as any).getAuthenticatorData())
+        : undefined,
+      publicKey: (credential.response as any).getPublicKey
+        ? base64url((credential.response as any).getPublicKey())
+        : undefined,
+      publicKeyAlgorithm: (credential.response as any).getPublicKeyAlgorithm?.(),
+      transports: (credential.response as any).getTransports?.(),
+    },
+  }
+}
